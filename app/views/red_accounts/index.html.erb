<p id="notice"><%= notice %></p>

<h1>Red Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Accountbalance</th>
      <th>Accountlimit</th>
      <th>Paymentdate</th>
      <th>Userid</th>
      <th>Integer</th>
      <th>Apr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @red_accounts.each do |red_account| %>
      <tr>
        <td><%= red_account.id %></td>
        <td><%= red_account.name %></td>
        <td><%= red_account.accountBalance %></td>
        <td><%= red_account.accountLimit %></td>
        <td><%= red_account.paymentDate %></td>
        <td><%= red_account.userID %></td>
        <td><%= red_account.integer %></td>
        <td><%= red_account.apr %></td>
        <td><%= link_to 'Show', red_account %></td>
        <td><%= link_to 'Edit', edit_red_account_path(red_account) %></td>
        <td><%= link_to 'Destroy', red_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Red Account', new_red_account_path %>
